<html>
  <head>
  </head>
  <body>
    <h1>Hello world</h1>
    <input type='button' value='comment' />
    <script src='https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.4.5/socket.io.min.js'></script>
    <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-beta1/jquery.min.js'></script>
    <script src='https://s3-ap-northeast-1.amazonaws.com/pushould-static-files/v0.0.1/pushould.min.js'></script>
    <script src='/sample.js'></script>
    <script>
      var pushould = new Pushould({ url: "<%= @url %>", client_token: "<%= @client_token %>" });
      var channel = pushould.subscribe('private area');
      channel.bind('send', function(msg) { console.log(msg) });
    </script>
  </body>
</html>
